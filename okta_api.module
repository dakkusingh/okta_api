<?php

/**
 * @file
 * Contains okta_api.module.
 */

function okta_api_update_8001() {
  $config = \Drupal::configFactory()->getEditable('okta_api.settings');
  $oldKey = $config->get('api_key');

  if (isset($oldKey)) {
    $config->set('okta_api_key', $oldKey);
    $config->clear('api_key');
    $config->save();
  }
}
